  <DataSets>
    <DataSet Name="R11_DD_Reconciliation_Dataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Agency">
            <Value>=Parameters!Agency.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        [Import ID], Status, [Status Date], ReportSetID, [DD Form Number], [Number of Payments Made], [DD Failure Code], Agency, [Initial Amount], [Core_Start of bracket], 
                         [Core refund], [Multiplier_Start of bracket], [Refund multiplier], [Core refund] * [Refund multiplier] AS [Refund owed]
FROM            R5_3_CancelledRGs_withTotals_RefundEnriched
WHERE        ([Status Date] BETWEEN @StartDate AND @EndDate) AND (Agency IN (@Agency))</CommandText>
      </Query>
